<!DOCTYPE html>
<html data-bs-theme="light" lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewData["Title"] - RPRENTAL</title>
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=ADLaM+Display&amp;display=swap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Abel&amp;display=swap">
    <link rel="stylesheet" href="~/css/Articles-Cards-images.css">
    <link rel="stylesheet" href="~/css/Footer-Dark-Multi-Column-icons.css">
    <link rel="stylesheet" href="~/css/styles.css">
</head>

<body class="bg-dark" style="font-family: Abel, sans-serif;">
    <nav class="navbar navbar-expand-md bg-dark" data-bs-theme="dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">RP HOTEL</a><button data-bs-toggle="collapse" class="navbar-toggler" data-bs-target="#navcol-1"><span class="visually-hidden">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
            <div class="collapse navbar-collapse" id="navcol-1">
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link active" href="#">Dashboard</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">Home</a></li>
                    <li class="nav-item dropdown ">
                        <a class="dropdown-toggle nav-link" aria-expanded="true" data-bs-toggle="dropdown" href="#">Management</a>
                        <div class="dropdown-menu text-dark bg-dark" data-bs-popper="none" style="background: rgb(207, 226, 255);">
                            <a class="dropdown-item" href="#">Room</a><a class="dropdown-item" href="#">Room Number</a><a class="dropdown-item" href="#">Amenity</a>
                            <div class="dropdown-divider"></div><a class="dropdown-item" href="#">Booking List</a>
                            <div class="dropdown-divider"></div><a class="dropdown-item" href="#">Register&nbsp;</a>
                        </div>
                    </li>
                </ul>
                <div class="text-center d-xxl-flex justify-content-center ms-auto justify-content-xxl-center align-items-xxl-end">
                    <div class="row text-center">
                        <div class="col text-center"><label class="col-form-label fw-bold text-light" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="left" title="Number of booking (s).">[ 5 ]</label></div>
                        <div class="col">
                            <ul class="navbar-nav ms-auto">
                                <li class="nav-item dropdown">
                                    <a class="dropdown-toggle nav-link" aria-expanded="false" data-bs-toggle="dropdown" href="#">Welcome, Rolimer Pono</a>
                                    <div class="dropdown-menu text-dark bg-dark" style="background: rgb(207, 226, 255);"><a class="dropdown-item" href="#">Login</a><a class="dropdown-item" href="#">Logout</a><a class="dropdown-item" href="#">Register</a></div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <div>
        <div class="carousel slide" data-bs-ride="carousel" data-bs-interval="4000" id="Carousel-Image">
            <div class="carousel-inner">
                <div class="carousel-item active" style="height: 620px;"><img class="w-100 d-block carousel-pic" src="~/img/Carousel/1.jpg" alt="Slide Image"></div>
                <div class="carousel-item" style="height: 620px;"><img class="w-100 d-block carousel-pic" src="~/img/Carousel/2.jpg" alt="Slide Image"></div>
                <div class="carousel-item" style="height: 620px;"><img class="w-100 d-block carousel-pic" src="~/img/Carousel/3.jpg" alt="Slide Image"></div>
            </div>
            <div><a class="carousel-control-prev" href="#Carousel-Image" role="button" data-bs-slide="prev"><span class="carousel-control-prev-icon"></span><span class="visually-hidden">Previous</span></a><a class="carousel-control-next" href="#Carousel-Image" role="button" data-bs-slide="next"><span class="carousel-control-next-icon"></span><span class="visually-hidden">Next</span></a></div>
            <div class="carousel-indicators"><button type="button" data-bs-target="#Carousel-Image" data-bs-slide-to="0" class="active"></button> <button type="button" data-bs-target="#Carousel-Image" data-bs-slide-to="1"></button> <button type="button" data-bs-target="#Carousel-Image" data-bs-slide-to="2"></button></div>
        </div>
        
    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


<body style="font-family: Abel, sans-serif;">
    <nav class="navbar navbar-expand-md sticky-top shadow">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">RP HOTEL</a>
            <button data-bs-toggle="collapse" class="navbar-toggler" data-bs-target="#navcol-1">
                <span class="visually-hidden">Toggle navigation</span>
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navcol-1">
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link active" href="#">Dashboard</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li class="nav-item dropdown">
                        <a class="dropdown-toggle nav-link" aria-expanded="true" data-bs-toggle="dropdown" href="#">Management</a>
                        <div class="dropdown-menu" data-bs-popper="none" style="background: rgb(207, 226, 255);">
                            <a class="dropdown-item" asp-controller="Room" asp-action="Index">Room</a>
                            <a class="dropdown-item" href="#">Room Number</a>
                            <a class="dropdown-item" href="#">Amenity</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="#">Booking List</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="#">Register</a>
                        </div>
                    </li>
                </ul>
                <div class="text-center d-xxl-flex justify-content-center ms-auto justify-content-xxl-center align-items-xxl-end">
                    <div class="row text-center">
                        <div class="col text-center">
                            <label class="col-form-label fw-bold text-light" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="left" title="Number of booking (s).">[ 5 ]</label>
                        </div>
                        <div class="col">
                            <ul class="navbar-nav ms-auto">
                                <li class="nav-item dropdown">
                                    <a class="dropdown-toggle nav-link" aria-expanded="false" data-bs-toggle="dropdown" href="#">Welcome, Rolimer Pono</a>
                                    <div class="dropdown-menu" style="background: rgb(207, 226, 255);">
                                        <a class="dropdown-item" href="#">Login</a>
                                        <a class="dropdown-item" href="#">Logout</a>
                                        <a class="dropdown-item" href="#">Register</a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </nav>

    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

